{
  "name": "startup",
  "version": "1.0.0",
  "author": "Mykhailo Holota",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start-helper": "webpack-dev-server --config config/webpack.config.development.js",
    "start": "cross-env PROJECT_ENV=production npm run start",
    "start:development": "cross-env PROJECT_ENV=development npm run start",
    "start:staging": "cross-env PROJECT_ENV=staging npm run start",
    "start:production": "cross-env PROJECT_ENV=production npm run start",
    "ui:start": "start-storybook -p 9001 -c src/ui/.storybook",
    "build": "webpack --config config/webpack.config.production.js",
    "build:development": "cross-env BABEL_ENV=production PROJECT_ENV=development yarn build",
    "build:staging": "cross-env BABEL_ENV=production PROJECT_ENV=staging yarn build",
    "build:production": "cross-env BABEL_ENV=production PROJECT_ENV=production yarn build",
    "clean": "rimraf dist",
    "analyzer": "cross-env NODE_ENV=production BABEL_ENV=production PROJECT_ENV=production webpack --config config/webpack.config.production.js --analyze",
    "lint:js": "eslint config src --ext .js --ext .jsx --ext .ts --ext .tsx",
    "lint:css": "stylelint 'src/**/*.scss'",
    "prettier": "prettier '{config,src}/**/*.{js,jsx,ts,tsx}' --write",
    "pre-commit": "lint-staged",
    "cypress": "cypress open"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint:js --fix --"
    ],
    "*.scss": [
      "yarn lint:css --fix --"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"

    }
  },
